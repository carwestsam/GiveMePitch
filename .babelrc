{
  "presets": [
    ["@babel/preset-env", {
      "modules": false,
      "targets": {
        "browsers": ["> 1%", "last 2 versions", "not ie <= 8"]
      }
    }]
  ],
  "plugins": [
            "babel-plugin-transform-vue-jsx",
            "@babel/plugin-transform-runtime",
            "minify-dead-code-elimination",
            "@babel/plugin-syntax-dynamic-import"]
}
